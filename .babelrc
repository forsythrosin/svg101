{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    ["react-css-modules", {
      "exclude": "node_modules",
      "filetypes": {
        ".scss": {
          "syntax": "postcss-scss"
        }
      },
      "generateScopedName": "[hash:base64:12]"
    }],
    "@babel/plugin-proposal-class-properties"
  ]
}
